  <template name="rxa" value="new Action1&lt;$CLAZZ$&gt;() {&#10;    @Override&#10;    public void call($CLAZZ$ $VAR_NAME$) {&#10;&#10;    }&#10;}, new Action1&lt;Throwable&gt;() {&#10;    @Override&#10;    public void call(Throwable throwable) {&#10;        $END$&#10;    }&#10;}" description="Rx Next + Error Actions" toReformat="true" toShortenFQNames="true">
    <variable name="CLAZZ" expression="complete()" defaultValue="" alwaysStopAt="true" />
